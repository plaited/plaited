{
  "name": "plaited",
  "version": "7.2.0",
  "license": "ISC",
  "engines": {
    "bun": ">= v1.2.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaited/plaited.git"
  },
  "bugs": {
    "url": "https://github.com/plaited/plaited/issues"
  },
  "homepage": "https://github.com/plaited/plaited/tree/main#readme",
  "bin": {
    "plaited": "./src/cli.ts"
  },
  "type": "module",
  "main": "./src/main.ts",
  "exports": {
    ".": "./src/main.ts",
    "./*.ts": "./src/*.ts",
    "./*.tsx": "./src/*.tsx",
    "./ui": "./src/ui.ts",
    "./ui/*.ts": "./src/ui/*.ts",
    "./ui/*.tsx": "./src/ui/*.tsx",
    "./jsx-dev-runtime": "./src/jsx-dev-runtime.ts",
    "./jsx-runtime": "./src/jsx-runtime.ts",
    "./testing": "./src/testing.ts",
    "./testing/*.ts": "./src/testing/*.ts",
    "./testing/*.tsx": "./src/testing/*.tsx",
    "./utils": "./src/utils.ts",
    "./utils/*.ts": "./src/utils/*.ts",
    "./utils/*.tsx": "./src/utils/*.tsx",
    "./workshop": "./src/workshop.ts",
    "./workshop/*.ts": "./src/workshop/*.ts",
    "./workshop/*.tsx": "./src/workshop/*.tsx",
    "./agent": "./src/agent.ts",
    "./agent/*.ts": "./src/agent/*.ts",
    "./acp": "./src/acp.ts",
    "./acp/*.ts": "./src/acp/*.ts"
  },
  "files": [
    "./.bin/**",
    "./.plaited.ts",
    "./src/**",
    "!./src/**/tests/*",
    "!./src/**/*.spec.@(tsx|ts)",
    "!./src/**/*.stories.@(tsx|ts)"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check": "bun run check:biome && bun run check:types && bun run check:package",
    "check:biome": "biome check",
    "check:package": "format-package --check",
    "check:types": "tsc --noEmit",
    "check:write": "biome check --write && format-package --write",
    "copy-css-types": "bash scripts/copy-css-types.sh",
    "dev": "bun --hot src/cli.ts dev src/ui",
    "prepare": "git rev-parse --git-dir > /dev/null 2>&1 && git config core.hooksPath .hooks || true",
    "test": "bun run test:stories src/ui && bun test",
    "test:acp": "docker compose -f docker-compose.test.yml run --rm acp-integration",
    "test:acp:build": "docker compose -f docker-compose.test.yml build",
    "test:playwright": "bunx playwright test",
    "test:stories": "bun src/cli.ts test",
    "test:stories-all": "bun run test:stories src/ui"
  },
  "lint-staged": {
    "*.(js|cjs|tsx|ts)": [
      "bunx @biomejs/biome check --write"
    ],
    "package.json": [
      "format-package -w"
    ]
  },
  "peerDependenciesMeta": {
    "@agentclientprotocol/sdk": {
      "optional": true
    },
    "@anthropic-ai/claude-agent-sdk": {
      "optional": true
    },
    "@modelcontextprotocol/sdk": {
      "optional": true
    },
    "@types/bun": {
      "optional": true
    },
    "axe-core": {
      "optional": true
    },
    "playwright": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "dependencies": {
    "@anthropic-ai/sandbox-runtime": "^0.0.23",
    "@huggingface/inference": "^4.13.5",
    "@plaited/development-skills": "0.4.1"
  },
  "peerDependencies": {
    "@agentclientprotocol/sdk": "^0.12.0",
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@types/bun": "^1.3.2",
    "axe-core": "^4.11.0",
    "playwright": "^1.56.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@agentclientprotocol/sdk": "0.12.0",
    "@anthropic-ai/claude-agent-sdk": "0.1.76",
    "@biomejs/biome": "2.3.6",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@modelcontextprotocol/sdk": "1.26.0",
    "@playwright/mcp": "0.0.54",
    "@playwright/test": "1.57.0",
    "@types/beautify": "0.0.3",
    "@types/bun": "1.3.5",
    "@types/node": "25.0.3",
    "@types/sinon": "21.0.0",
    "axe-core": "4.11.0",
    "beautify": "0.0.8",
    "@zed-industries/claude-code-acp": "0.12.6",
    "csstype": "3.2.3",
    "cz-conventional-changelog": "3.3.0",
    "format-package": "7.0.0",
    "lint-staged": "16.2.7",
    "playwright": "1.57.0",
    "sinon": "21.0.1",
    "typescript": "5.9.3",
    "typescript-language-server": "5.1.3",
    "zod": "4.2.1"
  }
}
