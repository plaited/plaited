{
  "name": "tic-tac-toe",
  "type": "module",
  "scripts": {
    "start": "ts-node-esm --experimentalSpecifierResolution node tic-tac-toe.ts",
    "test": "exit 0"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "files": [
      "src/**/*.spec.ts"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "dependencies": {
    "@plaited/plait": "workspace:*"
  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/node": "18.11.18",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  }
}
