{
  "name": "calculator",
  "type": "module",
  "scripts": {
    "build": "yarn build:dist & yarn build:public",
    "build:dist": "tsc --project tsconfig.json",
    "build:public": "esbuild src/islands/registry.ts --bundle --sourcemap --format=esm  --minify --target=es2022,chrome109 --outdir=public",
    "start": "yarn build && node dist/index.js",
    "test": "exit 0"
  },
  "dependencies": {
    "@plaited/island": "workspace:*",
    "@plaited/template": "workspace:*"
  },
  "devDependencies": {
    "@plaited/server": "workspace:*",
    "@tsconfig/node18": "1.0.1",
    "@types/node": "18.11.18",
    "esbuild": "0.17.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}