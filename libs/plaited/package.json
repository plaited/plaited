{
  "name": "plaited",
  "version": "6.0.0",
  "license": "ISC",
  "engines": {
    "node": ">= v18.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaited/plaited.git"
  },
  "bugs": {
    "url": "https://github.com/plaited/plaited/issues"
  },
  "homepage": "https://github.com/plaited/plaited/tree/main/libs/plaited#readme",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./hda": "./dist/hda.js",
    "./jsx-dev-runtime": "./dist/jsx-dev-runtime.js",
    "./jsx-runtime": "./dist/jsx-runtime.js",
    "./ssr": "./dist/ssr.js",
    "./utils": "./dist/utils.js",
    "./worker": "./dist/worker.js"
  },
  "files": [
    "./dist/**",
    "./jsx-runtime.js",
    "./jsx-dev-runtime.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build-watch": "bun run build --watch",
    "test": "bun run test-bun && npm run test-web",
    "test-bun": "bun test tests/bun/*",
    "test-bun-watch": "bun test tests/bun/* --watch",
    "test-web": "web-test-runner \"tests/web/*.spec.ts(x)?\"",
    "test-web-watch": "web-test-runner \"tests/web/*.spec.ts(x)?\" --watch"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@plaited/utils": "6.0.0"
  },
  "devDependencies": {
    "@plaited/rite": "workspace:*",
    "@web/dev-server": "0.4.2",
    "@web/dev-server-esbuild": "1.0.2",
    "@web/test-runner": "0.18.0",
    "@web/test-runner-core": "0.13.0",
    "@web/test-runner-playwright": "0.11.0"
  }
}
