{
  "name": "@plaited/storybook",
  "version": "5.4.1",
  "description": "Storybook plaited components renderer",
  "license": "ISC",
  "engines": {
    "node": ">= v18.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaited/plaited.git"
  },
  "bugs": {
    "url": "https://github.com/plaited/plaited/issues"
  },
  "homepage": "https://github.com/plaited/plaited/tree/main/libs-storybook/storybook#readme",
  "keywords": [
    "plaited",
