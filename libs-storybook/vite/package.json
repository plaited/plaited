{
  "name": "@plaited/storybook-vite",
  "version": "5.5.0",
  "description": "Storybook for Plaited and Vite: Develop Web Components in isolation with Hot Reloading.",
  "license": "ISC",
  "engines": {
    "node": ">= v18.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaited/plaited.git"
  },
  "bugs": {
    "url": "https://github.com/plaited/plaited/issues"
  },
  "homepage": "https://github.com/plaited/plaited/tree/main/libs-storybook/vite#readme",
  "keywords": [
    "plaited",
    "storybook",
