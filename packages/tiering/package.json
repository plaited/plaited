{
  "name": "@plaited/tiering",
  "version": "0.0.0",
  "engines": {
    "node": ">= v18.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plaited/plaited.git"
  },
  "homepage": "https://github.com/plaited/plaited/blob/main/packages/tiering/README.md",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "prepublishOnly": "yarn build",
    "test": "yarn build && ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "files": [
      "src/**/*.spec.*"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@plaited/node-utils": "workspace:*",
    "@plaited/template": "workspace:*",
    "@plaited/utils": "workspace:*",
    "arg": "5.0.2",
    "inquirer": "9.1.4",
    "postcss": "8.4.21",
    "postcss-combine-duplicated-selectors": "10.0.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.11.18",
    "ava": "5.1.0",
    "concat-stream": "2.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
