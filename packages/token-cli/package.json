{
  "name": "@plaited/token-cli",
  "version": "0.0.0",
  "engines": {
    "node": ">= v18.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plaited/plaited.git"
  },
  "homepage": "https://github.com/plaited/plaited/blob/main/packages/cli/README.md",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --watch --project tsconfig.json",
    "prepublishOnly": "yarn build",
    "test": "ava",
    "test:watch": "yarn test --watch --verbose"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "files": [
      "src/**/*.spec.*"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "types": "dist/lib/index.d.ts",
  "dependencies": {
    "@plaited/utils": "workspace:*",
    "csso": "5.0.5",
    "lodash-es": "4.17.21",
    "postcss": "8.4.21",
    "postcss-combine-duplicated-selectors": "10.0.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/beautify": "0.0.0",
    "@types/csso": "5.0.0",
    "@types/lodash-es": "4.17.6",
    "@types/sinon": "10.0.13",
    "ava": "5.1.0",
    "beautify": "0.0.8",
    "sinon": "15.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
